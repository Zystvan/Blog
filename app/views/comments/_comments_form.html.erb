<%= form_for(@comment) do |f| %>
  <%#= f.hidden_field :post_id, :autocomplete => 'off' %>
  <%= f.text_field :name, :autocomplete => 'off' %>
  <%= f.email_field :email, :autocomplete => 'off' %>
  <%= f.text_field :website, :autocomplete => 'off' %>
  <%= f.text_area :content %>
  <%= f.submit %>
<% end %>